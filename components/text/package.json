{
  "version": "0.0.2",
  "name": "@pixeen/text",
  "description": "Pixeen UI Text Component",
  "author": "Jeroen Gerits <jeroen@gerits.email>",
  "main": "dist/index",
  "types": "dist/index",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist -rf",
    "type-check": "tsc --noEmit",
    "build": "rimraf ./dist -rf && yarn build:types && yarn build:js && yarn build:readme",
    "build:types": "tsc -b tsconfig.build.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:readme": "rimraf ./README.md && readme -y -p ../../_template.md"
  },
  "dependencies": {
    "@pixeen/reset": "^0.0.2"
  },
  "devDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "readme-md-generator": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "babel": {
    "extends": "../../babel.config.js"
  },
  "gitHead": "c91315db890daad35e130dabb87b447c9dc7ce51"
}
