  "engines": {
    "npm": "~6.14.10",
    "node": "~14.15.4"
  },
  "homepage": "https://github.com/pixeen/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixeen/ui.git"
  },
  "main": "dist/index",
  "types": "dist/index",
  "license": "MIT",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "ui",
    "components",
    "library"
  ],
  "scripts": {
    "clean": "rimraf dist -rf",
    "type-check": "tsc --noEmit",
    "build": "rimraf ./dist -rf && yarn build:types && yarn build:js && yarn build:readme",
    "build:types": "tsc -b tsconfig.build.json",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "build:readme": "rimraf ./README.md && readme -y -p ../../_template.md"
  },
  "dependencies": {
    "@pixeen/reset": "^0.0.16"
  },
  "devDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "readme-md-generator": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "babel": {
    "extends": "../../babel.config.js"
  },
