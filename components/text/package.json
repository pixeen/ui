{
  "license": "MIT",
  "version": "0.0.35",
  "name": "@pixeen/text",
  "description": "Pixeen UI Text",
  "keywords": [
    "pixeen",
    "react",
    "typescript",
    "ui",
    "component"
  ],
  "scripts": {
    "clean": "rimraf dist -rf && rimraf -rf tsconfig.build.tsbuildinfo",
    "build": "yarn clean && yarn build:types && yarn build:js",
    "build:types": "tsc -b tsconfig.build.json",
    "build:js": "babel src --out-dir dist --extensions .ts,.tsx --source-maps inline",
    "build:readme": "rimraf ./README.md && readme -y -p ../../_template.md",
    "prepublishOnly": "yarn build && yarn build:readme"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@pixeen/reset": "^0.0.35"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "goober": "^2.0.28",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "babel": {
    "extends": "../../babel.config.js"
  },
  "author": {
    "name": "Jeroen Gerits",
    "email": "jeroen@gerits.email"
  },
  "bugs": {
    "url  ": "https://github.com/pixeen/ui/issues",
    "email": "jeroen@gerits.email"
  },
  "homepage": "https://github.com/pixeen/ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixeen/ui.git"
  }
}
