  "version": "0.0.31",
  "name": "@pixeen/text",
  "description": "Pixeen UI Text",
  "engines": {
    "npm": "~6.14.10",
    "node": "~14.15.4"
  },
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm dist -rf && rm -rf tsconfig.build.tsbuildinfo",
    "build": "yarn clean && yarn build:types && yarn build:js && yarn build:readme",
    "build:types": "tsc -b tsconfig.build.json",
    "build:js": "babel src --out-dir dist --root-mode upward --extensions .ts,.tsx --source-maps inline",
    "build:readme": "rimraf ./README.md && readme -y -p ../../_template.md",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@pixeen/reset": "^0.0.31"
  },
  "devDependencies": {
    "goober": "^2.0.28",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.3"
  },
  "babel": {
    "extends": "../../babel.config.js"
  },
